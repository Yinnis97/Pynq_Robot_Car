
#include "Functions.h"

int main()
{
    init_platform();

    GPIO_Initialize();
    MPU_Initialize();
    PWM_Initialize();

    u32	ReadValue=0;
    while(1)
    {
    	ReadValue = XGpio_DiscreteRead(&gpio_in, SWS_CHANNEL);
    	printf("Value : %ld\n\r",ReadValue);
    	sleep(1);
    }

    Main_Loop();

    return EXIT_FAILURE;

	for(;;);
}


